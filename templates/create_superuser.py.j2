from django.contrib.auth.models import User
from users.models import Token
if not User.objects.filter(username='{{ SUPERUSER_USER }}'):
   u=User.objects.create_superuser('{{ SUPERUSER_USER }}', '{{ SUPERUSER_EMAIL }}', '{{ SUPERUSER_PASSWORD }}')
   Token.objects.create(user=u, key='{{ SUPERUSER_API_TOKEN }}')
